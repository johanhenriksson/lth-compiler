
int main() {
    assert(3.0 < 5.0);
    except(3.0 > 5.0);

    assert(5.0 > 3.0);
    except(5.0 < 3.0);

    assert(3.0 == 3.0);
    except(3.0 == 5.0);

    assert(3.0 != 5.0);
    except(3.0 != 3.0);

    assert(1.0 <= 2.0);
    assert(2.0 <= 2.0);
    except(3.0 <= 2.0);

    assert(2.0 >= 1.0);
    assert(2.0 >= 2.0);
    except(1.0 >= 2.0);

    return 0;
}
