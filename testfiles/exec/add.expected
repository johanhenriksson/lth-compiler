; Library Functions
declare void @print(i64)
declare i64 @read()
declare i64 @truncate(double)
declare double @promote(i64)
declare void @assert(i1)
declare void @except(i1)

define i64 @main() {
%f = alloca double
%_t1 = fadd double 3.5, 3.0
store double %_t1, double* %f
%i = alloca i64
%_t2 = add i64 1, 2
store i64 %_t2, i64* %i
ret i64 0
}

